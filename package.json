{
  "name": "@ctrl/plex",
  "version": "0.0.0-placeholder",
  "description": "plex api client in typescript",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "scttcper/plex",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "plex",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "test": "jest --runInBand",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "claim-server": "ts-node scripts/claim-server.ts",
    "add-media": "ts-node scripts/add-media.ts",
    "test-cleanup": "ts-node scripts/test-cleanup.ts"
  },
  "dependencies": {
    "@ctrl/mac-address": "^2.1.2",
    "got": "^11.8.2",
    "p-any": "^3.0.0",
    "type-fest": "^1.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@babel/preset-typescript": "7.13.0",
    "@ctrl/eslint-config": "2.0.8",
    "@jest/globals": "27.0.3",
    "@types/lodash": "4.14.170",
    "@types/node": "15.12.1",
    "@types/puppeteer": "5.4.3",
    "@types/xml2js": "0.4.8",
    "jest": "27.0.4",
    "jest-junit": "12.1.0",
    "lodash": "4.17.21",
    "puppeteer": "10.0.0",
    "ts-node": "10.0.0",
    "typedoc": "0.21.0-beta.2",
    "typescript": "4.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageProvider": "v8"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10"
  }
}
