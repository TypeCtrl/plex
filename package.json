{
  "name": "@ctrl/plex",
  "version": "0.0.0-placeholder",
  "description": "plex api client in typescript",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "typectrl/plex",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "plex",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, . --format codeframe",
    "lint:fix": "eslint --fix --ext .js,.ts, . --format codeframe",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "claim-server": "ts-node scripts/claim-server.ts",
    "add-media": "ts-node scripts/add-media.ts",
    "test-cleanup": "ts-node scripts/test-cleanup.ts"
  },
  "dependencies": {
    "@ctrl/mac-address": "^2.1.2",
    "got": "^11.8.1",
    "p-any": "^3.0.0",
    "type-fest": "^0.20.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-typescript": "7.12.7",
    "@ctrl/eslint-config": "1.2.9",
    "@types/lodash": "4.14.167",
    "@types/node": "14.14.19",
    "@types/puppeteer": "5.4.2",
    "@types/xml2js": "0.4.7",
    "@jest/globals": "26.6.2",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "lodash": "4.17.20",
    "puppeteer": "5.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10"
  }
}
